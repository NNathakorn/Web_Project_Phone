          <h1 class="h3 mb-3 fw-normal">Sign Up</h1>
      
          <% if(errors != null && errors.length>0){ %>
              <% for(let i = 0; i < errors.length; i++){ %>
                  <p class = "alert alert-danger">
                      <%= errors[i] %>
                  </p>
              <% } %>
          <% } %>
      
          <div style=" margin-bottom: 20px;">
            <div>
              <label for="floatingInput">Email address</label>
              <input type="email" name="email" class="form-control" id="floatingInput" placeholder="name@example.com" style="height: 20px; width: 300px; border:solid black; border-radius: 20px;">
            </div>
            <div style="margin: 0;">
              <label for="floatingPassword">Password</label>
              <input type="password" name="password" class="form-control" id="floatingPassword" placeholder="Password" style="height: 20px; width: 300px; margin: 0; border:solid black; border-radius: 20px;">
            </div>
            <div>
              <label for="floatingUsername">Username</label>
              <input type="username" name="username" class="form-control" id="floatingUsername" placeholder="Username" style="height: 20px; width: 300px; border:solid black; border-radius: 20px;">
            </div>
          </div>
          <button class="w-100 btn btn-lg btn-primary" type="submit">Register</button>
        </div>
      </form>
    </main>


    
